/*!40000 ALTER TABLE `posts` ENABLE KEYS */;
UNLOCK TABLES;

--
-- Table structure for table `refit_pages`
--

DROP TABLE IF EXISTS `refit_pages`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!50503 SET character_set_client = utf8mb4 */;
CREATE TABLE `refit_pages` (
  `id` int NOT NULL AUTO_INCREMENT,
  `news` text,
  `schedule` text,
  `loc` text,
  `about_me` text,
  `user_id` int NOT NULL,
  `created_at` datetime DEFAULT CURRENT_TIMESTAMP,
  `updated_at` datetime DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `playlist_id` int DEFAULT NULL,
  `loc_picture` varchar(255) DEFAULT NULL,
  `zoom_picture` varchar(255) DEFAULT NULL,
  `banner1_picture` varchar(255) DEFAULT NULL,
  `banner2_picture` varchar(255) DEFAULT NULL,
  `about_me_picture` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `fk_refit_page_users1_idx` (`user_id`),
  KEY `fk_refit_page_playlists1_idx` (`playlist_id`),
  CONSTRAINT `fk_refit_page_playlists1` FOREIGN KEY (`playlist_id`) REFERENCES `playlists` (`id`),
  CONSTRAINT `fk_refit_page_users1` FOREIGN KEY (`user_id`) REFERENCES `users` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=5 DEFAULT CHARSET=utf8;
/*!40101 SET character_set_client = @saved_cs_client */;

--
-- Dumping data for table `refit_pages`
--

LOCK TABLES `refit_pages` WRITE;
